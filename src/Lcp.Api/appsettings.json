{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "VaultName": "{key_vault_name}",
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ConnectionString": "Server={server_name},1433;Database={database_name};Authentication=Active Directory Interactive"
  },
  "Fortify": {
    "Authority": "{authority}",
    "ApiScope": "{api_scope}"
  },
  "Events": {
    "ConnectionString": "{key_vault_secret}",
    "TopicName": "{topic_name}",
    "SubscriptionName": "{subscription_name}",
    "DelayTime": "0:0:10"
  }
}
